<%
  breadcrumb t('dashboard') => nil
%>
<%= t('my_requests') %>
<% @moves.each do |move| %>
  <%= link_to move do %>
    <%= move.from_group.age %>
    <%= move.from_group.location.name %>
    <%= move.to_groups.each do |group| %>
      <% next unless group %>
      --> <%= group.location.name %>
      <br>
    <% end %>
  <% end %>
<% end %>
